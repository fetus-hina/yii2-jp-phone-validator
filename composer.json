{
    "name": "jp3cki/yii2-jp-phone-validator",
    "description": "Phone number validator for Yii Framework2",
    "license": "MIT",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "validator",
        "phone",
        "tel",
        "telephone",
        "japan"
    ],
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "ext-pcre": "*",
        "yiisoft/yii2": "^2.0.53"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.47",
        "php-curl-class/php-curl-class": "^11.1.0",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpoffice/phpspreadsheet": "^3.10.0",
        "phpunit/phpunit": ">=10.5.35",
        "squizlabs/php_codesniffer": "^3.13.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "jp3cki\\yii2\\jpphone\\": "src",
            "jp3cki\\yii2\\jpphone\\test\\": "test/src"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "yiisoft/yii2-composer": true
        },
        "platform": {
            "php": "8.1.30"
        }
    },
    "extra": {
        "bootstrap": "jp3cki\\yii2\\jpphone\\internal\\Bootstrap"
    }
}

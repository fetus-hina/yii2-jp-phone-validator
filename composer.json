{
    "name": "jp3cki/yii2-jp-phone-validator",
    "type": "yii2-extension",
    "description": "Phone number validator for Yii Framework2",
    "keywords": ["yii2", "extension", "validator", "phone", "tel", "telephone", "japan"],
    "license": "MIT",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "ext-pcre": "*",
        "yiisoft/yii2": "^2.0.51"
    },
    "require-dev": {
        "php-curl-class/php-curl-class": "^11.0.0",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpoffice/phpspreadsheet": "^3.3.0",
        "phpunit/phpunit": ">=10.5.35",
        "squizlabs/php_codesniffer": "^3.10.3"
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\yii2\\jpphone\\": "src",
            "jp3cki\\yii2\\jpphone\\test\\": "test/src"
        }
    },
    "extra": {
        "bootstrap": "jp3cki\\yii2\\jpphone\\internal\\Bootstrap"
    },
    "config": {
        "platform": {
            "php": "8.1.30"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}

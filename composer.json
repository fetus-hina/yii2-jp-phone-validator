{
    "name": "jp3cki/yii2-jp-phone-validator",
    "type": "yii2-extension",
    "description": "Phone number validator for Yii Framework2",
    "keywords": ["yii2", "extension", "validator", "phone", "tel", "telephone", "japan"],
    "license": "MIT",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php": ">= 7.1.0",
        "ext-pcre": "*",
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "php-curl-class/php-curl-class": "^9.1.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpoffice/phpspreadsheet": "^1.10.0",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "^3.6.0"
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\yii2\\jpphone\\": "src",
            "jp3cki\\yii2\\jpphone\\test\\": "test/src"
        }
    },
    "extra": {
        "bootstrap": "jp3cki\\yii2\\jpphone\\internal\\Bootstrap"
    },
    "config": {
        "platform": {
            "php": "7.1.33"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
